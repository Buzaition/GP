{
  "builds": [
    {
      "src": ".", 
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.12",  
        "env": {
          "FLASK_ENV": "production"
        }
      }
    }
     { "src": "*.html", "use": "@vercel/static" }
    { "src": "*.py", "use": "@vercel/python" }
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "/API.app" 
    }
  ]
}
